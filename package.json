{
  "name": "node-typescript-advanced-template",
  "version": "1.1.1",
  "main": "src/server.ts",
  "repository": "https://github.com/DestroyeerU/node-typescript-advanced-template.git",
  "author": "DestroyeerU <destroyeeru18@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions .js,.ts --out-dir dist --copy-files --no-copy-ignored",
    "start": "node ./dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --no-notify --ignore-watch node_modules src/server.ts",

    "migrate:reset": "dotenv -e .env -- prisma migrate down `20200629145634-create-user` --experimental",
    "migrate:reset:test": "dotenv -e .env.test -- prisma migrate down `20200629145634-create-user` --experimental",

    "migrate": "prisma format && dotenv -e .env -- prisma migrate save -c --experimental && dotenv -e .env -- prisma migrate up --experimental && dotenv -e .env -- prisma generate",
    "migrate:test": "dotenv -e .env.test -- prisma migrate save -c --experimental && dotenv -e .env.test -- prisma migrate up --experimental && dotenv -e .env.test -- prisma generate",

    "pretest": "yarn migrate:reset:test && yarn migrate:test",
    "test": "NODE_ENV=test dotenv -e .env.test -- jest"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.7",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.template": "^4.5.0",
    "mongoose": "^5.9.15",
    "multer": "^1.4.2",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@prisma/cli": "^2.0.0-beta.7",
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.20",
    "@types/multer": "^1.4.3",
    "@types/supertest": "^2.0.9",
    "@types/yup": "^0.29.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
